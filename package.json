  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "yarn prisma:generate && node ./dist",
    "start:dev": "NODE_ENV=development yarn prisma:generate && nodemon",
    "prisma:generate": "yarn prisma:migrate && prisma2 generate",
    "prisma:migrate": "yarn prisma:save && prisma2 migrate up --experimental",
    "prisma:save": "prisma2 migrate save --name init --experimental",
    "test": "NODE_ENV=test jest"
  },
  "devDependencies": {
    "@prisma/cli": "^2.0.0-beta.5",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.1",
    "@types/request": "^2.48.5",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "gts": "^2.0.2",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.5",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^1.34.10",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "routing-controllers": "^0.8.1",
    "supertest": "^4.0.2",
    "winston": "^3.2.1"
  }
}
